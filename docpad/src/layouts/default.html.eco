<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width,initial-scale=1,minimum-scale=1,maximum-scale=1">
	<title><%=@site.title%> | Journal | <%=@formatDate(@document.date, "MMMM YYYY")%> | <%= @document.title %></title>
	<link rel="stylesheet" href="/css/unsemantic-grid-responsive-no-ie7.css">
	<link rel="stylesheet" href="/css/main.css">
</head>
<body class="<%= @document.layout %>">
	<div class="grid-parent">
		<header class="grid-container">
			<div class="grid-100">
				<h1><%=@site.title%></h1>
			</div>
			<ul class="mainMenu">
				<li class="grid-10"><a href="/">Home</a></li>
				<li class="grid-15"><a href="/journal/">Scribblings</a></li>
				<li class="grid-10"><a href="/bunkercam/">Playground</a></li>
			</ul>
		</header>
		<%- @content %>
		<footer class="grid-container">
			<div class="copyright grid-80">&copy; Copyright <%=@site.author%> 2007-<%=@formatDate(@ddate, "YYYY")%></div>
			<div class="social grid-20">
				<ul>
					<li><a href="http://www.facebook.com/crawlem/"><img src="/images/fb.png" width="40" height="40" alt="Facebook"></a></li>
					<li><a href="http://www.twitter.com/crawlem/"><img src="/images/twitter.png" width="40" height="40" alt="Twitter"></a></li>
					<li><a href="mailto:mark@crawleyweb.co.uk"><img src="/images/mail.png" width="40" height="40" alt="Email"></a></li>
				</ul>
			</div>
		</footer>
	</div>
	<!-- //-beg- concat_js -->
	<script src="/js/main.js"></script>
	<!-- //-end- concat_js -->
	<script>
		(function(b,o,i,l,e,r){b.GoogleAnalyticsObject=l;b[l]||(b[l]=
		function(){(b[l].q=b[l].q||[]).push(arguments)});b[l].l=+new Date;
		e=o.createElement(i);r=o.getElementsByTagName(i)[0];
		e.src='//www.google-analytics.com/analytics.js';
		r.parentNode.insertBefore(e,r)}(window,document,'script','ga'));
		ga('create','UA-6098332-2');ga('send','pageview');
	</script>
</body>
</html>